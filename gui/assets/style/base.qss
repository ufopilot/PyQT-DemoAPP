* {{ 
	color: #fff; 
	font-size: 14px;
}}

#mainFrame{{
	border: 1px solid #000;
	background:  {main_bg};
}}

/*******************************************/
/* HEADER                                  */
/*******************************************/
TitleBar{{
	background:  {header_bg};
	border-bottom: 0px solid {controller_bg};
	
}}

#logo{{
	background:  {header_bg};
}}

#mainHeader{{
	background:  {header_bg};
}}

#mainHeader QLabel{{
	background:  {header_bg};
}}

#appTitle{{
	font-weight: bold;
}}

#appDescription, #appTitle{{
	color: #8a95aa;
}}

#windowicons QLabel{{ background:  {header_bg}; }}

#windowicons{{ border: none}}

#windowicons QPushButton#restorewindow:hover,
#windowicons QPushButton#maximizewindow:hover,
#windowicons QPushButton#minimizewindow:hover{{ 
	border: none;
	background: {main_bg};
}}
#windowicons QPushButton#closewindow:hover{{ 
	background: red;
}}

QLabel#appLogo{{
	background:  {header_bg};
}}
/*******************************************/
/* FOOTER                                  */
/*******************************************/
FooterBar{{
	background:  {header_bg};
	border-bottom: 0px solid {controller_bg};
	
}}

#mainFooter QFrame{{
	border: none;
}}

#mainFooter{{
	background:  {footer_bg};
}}

/*******************************************/
/* WINDOWACTIONS TOPICONS                  */
/*******************************************/
#windowicons{{ border: none}}
#windowicons QPushButton:hover{{ 
	border: none;
	background: {controller_bg};
}}
#windowicons QPushButton#closewindow:hover{{ 
	background: red;
}}
