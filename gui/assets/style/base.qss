/*******************************************/
/* BUTTONS                                 */
/*******************************************/
QPushButton{{
    background: {main_bg};
}}

QPushButton:hover{{
    background: {controller_bg};
}}
/*******************************************/
/* SCROLLBAR                               */
/*******************************************/
QScrollBar {{
    background: {scrollbar_bg};
    width: 8px;   
}}

QScrollBar::handle {{
    background:  {scrollbar_handle_bg};
}}

QScrollBar::add-line:vertical, 
QScrollBar::add-line:horizontal  {{
    border: none;
    background: none;
}}

QScrollBar::sub-line:vertical, 
QScrollBar::sub-line:horizontal  {{
    border: none;
    background: none;
  }}

QScrollBar::handle:hover:vertical, 
QScrollBar::handle:hover:horizontal {{
    background: {scrollbar_hover_bg};
    border-radius: 3px;
}}
/*******************************************/
/* TREEVIEW                                */
/*******************************************/
QTreeView, QTreeWidget{{
    alternate-background-color: {table_alternate};
	background:  {main_bg};
	border: none;
}}

QTreeView::Item{{
    height:20px;
}}

QHeaderView::section, QHeaderWidget::section {{
    color: #37c886;
	font-weight: bold;
    background: {main_bg};
	padding: 2px;	
	border: none;
	border-bottom: 1px solid {header_bg};
}}
/*******************************************/
/* TOOLTIP                            */
/*******************************************/
QToolTip {{ 
	background-color: {controller_bg}; 
	color: #fff; 
	border: 1px solid #37c886; 
	border-radius: 3px;
}}

/*******************************************/
/* LINEEDIT                            */
/*******************************************/
QLineEdit{{
    background-color: {lineedit_bg};
    color: #dce1ec;
}}

/*******************************************/
/* TABS                                    */
/*******************************************/
QTabWidget::pane {{
	border: 1px solid {tab_pane_bg};
	top:-1px;
	background: {main_bg};
}}

QTabBar::tab {{
	background: {controller_bg};
	border: 1px solid {controller_bg};
	padding: 8px;
	color: #37c886;
	font-weight: bold;
	width: 117px;
  }}
QTabBar::scroller {{width: 0px;}}

QTabBar::tab:selected {{
	background: {main_bg};
	margin-bottom: -1px;
	color: #dce1ec;
	border-bottom: 2px solid #37c886;;
}}


/*******************************************/
/* Theming Pane                            */
/*******************************************/
#panel4 QTabWidget::pane {{
	border: 1px solid {tab_pane_bg};
	top:-1px;
	background: transparent;
}}

#panel4 QTabWidget::pane QLabel {{
	color: #fff;
}}


/*******************************************/
/* NAVIGATION MENU                         */
/*******************************************/
#contentMenu QTreeWidget{{
	font-weight: bold;
}}

#contentMenu QTreeWidget::Item{{
    height:30px;
	font-weight: bold;
	border-bottom:0px solid;
	/*padding-top: 5px;*/
	padding-left: 10px;
	margin-right: 10px;
}}

#contentMenu QTreeWidget::Item:selected{{
	/*background: {controller_bg};*/
	color: #fff;	

}}

#contentMenu QTreeWidget::item:hover {{
    background: {controller_bg};
}}

#contentMenu QTreeWidget::branch::vertical{{
	background: {main_bg};
}}
