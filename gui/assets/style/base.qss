	  background: #272c36;
	  color: #fff;
}}
/*******************************************/
/* SCROLLAREA                            */
/*******************************************/
QScrollArea, #scrollArea2, #scrollArea1, #scrollArea2 QFrame, #scrollArea1 QFrame{{
	border: none
}}

#scrollAreaWidgetContents{{
    background: {main_bg};
    border: 1px solid {main_bg};
}}
#scrollAreaWidgetContents2{{
    background: {main_bg};
    border: 1px solid {main_bg};
}}
/*******************************************/
/* TABLE                            */
/*******************************************/
QTableView{{
	alternate-background-color: #191e29;
    border: 1px solid #222;
    color: #dce1ec;
	background:  {main_bg};
}}

QTableCornerButton::section{{
    background-color: rgb(19, 22, 31);
    border-bottom: 1px solid #222;
    border-right: 1px solid #222;
    color: #8a95aa;
}}

