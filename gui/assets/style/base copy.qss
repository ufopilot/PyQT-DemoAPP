* {{ 
	color: {panel_lighter_text}; 
	font-size: 14px;
}}

#mainFrame{{
	border: 1px solid #000;
	background:  {panel_bg};
}}

/*******************************************/
/* HEADER                                  */
/*******************************************/
TitleBar{{
	background:  {header_bg};
	border-bottom: 0px solid {controller_bg};
	
}}

#logo{{
	background:  {header_bg};
	
}}

#mainHeader{{
	background:  {header_bg};
	color: {header_lighter_text}
}}

#mainHeader QLabel{{
	background:  {header_bg};
}}

#appTitle{{
	font-weight: bold;
}}

#appDescription, #appTitle{{
	color: {header_lighter_text};
}}

#windowicons QLabel{{
	background:  {header_bg}; 
}}

#windowicons{{ border: none}}
#windowicons QPushButton{{
	background: transparent;
	border: none;
}}
#windowicons QPushButton#restorewindow:hover,
#windowicons QPushButton#maximizewindow:hover,
#windowicons QPushButton#minimizewindow:hover{{ 
	border: none;
	background: {header_lighter_bg};
}}

#windowicons QPushButton#closewindow:hover{{ 
	background: red;
}}

QLabel#appLogo{{
	background:  {header_bg};
}}
/*******************************************/
/* FOOTER                                  */
/*******************************************/
#mainFooter QFrame{{
	border: none;
}}

#mainFooter{{
	background:  {footer_bg};
}}

#footerActions QPushButton{{ 
	border: none;
	background: {footer_bg};
}}

#footerActions QPushButton:hover{{
    background: {footer_lighter_bg};
}}


/*******************************************/
/* WINDOWACTIONS TOPICONS                  */
/*******************************************/
#windowicons{{ border: none}}
#windowicons QPushButton:hover{{ 
	border: none;
	background: {panel_bg};;
}}
#windowicons QPushButton#closewindow:hover{{ 
	background: red;
}}
