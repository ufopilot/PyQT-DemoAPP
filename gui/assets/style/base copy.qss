#controllerPanel2 QPushButton:hover, 
#controllerPanel3 QPushButton:hover,
#controllerPanel4 QPushButton:hover, 
#controllerPanel5 QPushButton:hover, 
#controllerPanel6 QPushButton:hover{{
	background: {controller_lighter_bg};
}}

LabelHorizontal, LabelVertical, #controllerPanel5 QLabel{{
	font-weight: bold;
	color: #d7c245;
	font-size: 16px;
	padding-left: 20px;
}}

#panel1, #panel2, #panel3, #panel5, #panel6{{
 	background: {panel_bg};
	color: #fff;
}}

#controllerPanel5{{
    border-bottom: 1px solid {header_bg};
}}
#controllerPanel1, #controllerPanel2{{
    border-left: 1px solid {header_bg};
}}

#controllerPanel3, #controllerPanel4, #controllerPanel5, #controllerPanel6{{
    border-right: 1px solid {header_bg};
}}

#panel6{{
    border-right: 1px solid {header_bg};
}}

#controllerPanel1 QFrame,
#controllerPanel2 QFrame,
#controllerPanel3 QFrame,
#controllerPanel4 QFrame,
#controllerPanel5 QFrame{{
	border: none;
}}

/*******************************************/
/* BUTTONS                                 */
/*******************************************/
QPushButton{{
	padding: 5px;
    background: {controller_bg};
	border: 1px solid {controller_lighter_bg};
}}

 QPushButton:hover{{
    background: {controller_lighter_bg};
	border: 1px solid {controller_darker_bg};
}}


/*******************************************/
/* SCROLLBAR                               */
/*******************************************/
QScrollBar {{
    background: {panel_lighter_bg};
    width: 8px;   
}}

QScrollBar::handle {{
    background:  {panel_darker_bg};
}}

QScrollBar::add-line:vertical, 
QScrollBar::add-line:horizontal  {{
    border: none;
    background: none;
}}

QScrollBar::sub-line:vertical, 
QScrollBar::sub-line:horizontal  {{
    border: none;
    background: none;
  }}

QScrollBar::handle:hover:vertical, 
QScrollBar::handle:hover:horizontal {{
    /*background: {panel_bg};*/
    border-radius: 3px;
}}
/*******************************************/
/* TREEVIEW                                */
/*******************************************/
QTreeView, QTreeWidget{{
    alternate-background-color: {panel_darker_bg};
	background:  {panel_bg};
	border: none;
}}

QTreeView::Item{{
    height:20px;
}}

QHeaderView::section, QHeaderWidget::section {{
    color: #37c886;
	font-weight: bold;
    background: {panel_bg};
	padding: 2px;	
	border: none;
	border-bottom: 1px solid {header_bg};
}}
/*******************************************/
/* TOOLTIP                            */
/*******************************************/
QToolTip {{ 
	background-color: {controller_bg}; 
	color: #fff; 
	border: 1px solid #37c886; 
	border-radius: 3px;
}}

/*******************************************/
/* LINEEDIT                            */
/*******************************************/
/*
QLineEdit{{
    background-color: {panel_bg};
    color: #dce1ec;
}}
*/
QLineEdit {{
	background-color: {lineedit_bg_color};
	border-radius: {lineedit_radius}px;
	border: {lineedit_border_size}px solid transparent;
	padding-left: 10px;
    padding-right: 10px;
	selection-color: {lineedit_selection_color};
	selection-background-color: {lineedit_context_color};
    color: {lineedit_color};
}}
QLineEdit:focus {{
	border: {lineedit_border_size}px solid {lineedit_context_color};
    background-color: {lineedit_bg_color_active};
	color: {lineedit_bg_color};
}}

/*******************************************/
/* ToolBox                                 */
/*******************************************/
QToolBox::tab {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #E1E1E1, stop: 0.4 #DDDDDD,
                                stop: 0.5 #D8D8D8, stop: 1.0 #D3D3D3);
    border-radius: 5px;
    color: darkgray;
}

QToolBox::tab:selected { /* italicize selected tabs */
    font: italic;
    color: white;
}
