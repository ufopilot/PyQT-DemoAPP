/*******************************************/
/* PANELS                                  */
/*******************************************/

#controllerPanel1, 
#controllerPanel2, 
#controllerPanel3,
#controllerPanel4, 
#controllerPanel5, 
#controllerPanel6{
 	background: var(controller_bg);
	/*border-top: 1px solid var(panel_bg);
	border-bottom: 1px solid var(controller_bg);
	*/
    border-right: 1px solid var(panel_bg);
	
}

#controllerPanel1 QPushButton, 
#controllerPanel2 QPushButton, 
#controllerPanel3 QPushButton,
#controllerPanel4 QPushButton, 
#controllerPanel5 QPushButton, 
#controllerPanel6 QPushButton{
	background: transparent;
	border: none;
}

#controllerPanel1 QPushButton:hover, 
#controllerPanel2 QPushButton:hover, 
#controllerPanel3 QPushButton:hover,
#controllerPanel4 QPushButton:hover, 
#controllerPanel5 QPushButton:hover, 
#controllerPanel6 QPushButton:hover{
	background: var(controller_lighter_bg);
}

LabelHorizontal, LabelVertical, #controllerPanel5 QLabel{
	font-weight: bold;
	color: #d7c245;
	font-size: 16px;
	padding-left: 20px;
}

#panel1, #panel2, #panel3, #panel5, #panel6{
 	background: var(panel_bg);
	color: #fff;
}

#controllerPanel5{
    border-bottom: 1px solid var(header_bg);
}
#controllerPanel1, #controllerPanel2{
    border-left: 1px solid var(header_bg);
}

#controllerPanel3, #controllerPanel4, #controllerPanel5, #controllerPanel6{
    border-right: 1px solid var(header_bg);
}

#panel6{
    border-right: 1px solid var(header_bg);
}

#controllerPanel1 QFrame,
#controllerPanel2 QFrame,
#controllerPanel3 QFrame,
#controllerPanel4 QFrame,
#controllerPanel5 QFrame{
	border: none;
}

/*******************************************/
/* BUTTONS                                 */
/*******************************************/
QPushButton{
	padding: 5px;
    background: var(controller_bg);
	border: 1px solid var(controller_darker_bg);
	color: var(controller_text);
}

 QPushButton:hover{
    background: var(controller_lighter_bg);
	border: 1px solid var(controller_darker_bg);
}


/*******************************************/
/* SCROLLBAR                               */
/*******************************************/
QScrollBar {
    background: var(panel_lighter_bg);
    width: 8px;   
}

QScrollBar::handle {
    background:  var(panel_darker_bg);
}

QScrollBar::add-line:vertical, 
QScrollBar::add-line:horizontal  {
    border: none;
    background: none;
}

QScrollBar::sub-line:vertical, 
QScrollBar::sub-line:horizontal  {
    border: none;
    background: none;
  }

QScrollBar::handle:hover:vertical, 
QScrollBar::handle:hover:horizontal {
    /*background: var(panel_bg);*/
    border-radius: 3px;
}
/*******************************************/
/* TREEVIEW                                */
/*******************************************/
QTreeView, QTreeWidget{
    alternate-background-color: var(panel_darker_bg);
	background:  var(panel_bg);
	border: none;
}

QTreeView::Item{
    height:20px;
}

QHeaderView::section, QHeaderWidget::section {
    color: #37c886;
	font-weight: bold;
    background: var(panel_bg);
	padding: 2px;	
	border: none;
	border-bottom: 1px solid var(header_bg);
}
