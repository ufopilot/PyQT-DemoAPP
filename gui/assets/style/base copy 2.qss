* { 
	color: var(panel_lighter_text); 
	font-size: 14px;
}

#mainFrame{
	border: 1px solid #000;
	background:  var(panel_bg);
}

/*******************************************/
/* HEADER                                  */
/*******************************************/
TitleBar{
	background:  var(header_bg);
	border-bottom: 0px solid var(controller_bg);
	
}

#logo{
	background:  var(header_bg);
	
}

#mainHeader{
	background:  var(header_bg);
	color: var(header_lighter_text)
}

#mainHeader QLabel{
	background:  var(header_bg);
}

#appTitle{
	font-weight: bold;
}

#appDescription, #appTitle{
	color: var(header_lighter_text);
}

#windowicons QLabel{
	background:  var(header_bg); 
}

#windowiconsvar{
	border: none
}
#windowicons QPushButton{
	background: transparent;
	border: none;
}
#windowicons QPushButton#restorewindow:hover,
#windowicons QPushButton#maximizewindow:hover,
#windowicons QPushButton#minimizewindow:hover{ 
	border: none;
	background: var(header_lighter_bg);
}

#windowicons QPushButton#closewindow:hover{ 
	background: red;
}

QLabel#appLogo{
	background:  var(header_bg);
}
/*******************************************/
/* FOOTER                                  */
/*******************************************/
#mainFooter QFrame{
	border: none;
}

#mainFooter{
	background:  var(footer_bg);
}

#footerActions QPushButton{ 
	border: none;
	background: var(footer_bg);
}

#footerActions QPushButton:hover{
    background: var(footer_lighter_bg);
}


/*******************************************/
/* WINDOWACTIONS TOPICONS                  */
/*******************************************/
#windowiconsvar{
	border: none
}
#windowicons QPushButton:hover{ 
	border: none;
	background: var(panel_bg);;
}
#windowicons QPushButton#closewindow:hover{ 
	background: red;
}
