/*******************************************/
/* TOOLTIP                            */
/*******************************************/
QToolTip { 
	background-color: var(controller_bg); 
	color: #fff; 
	border: 1px solid #37c886; 
	border-radius: 3px;
}

/*******************************************/
/* LINEEDIT                            */
/*******************************************/
/*
QLineEdit{
    background-color: var(panel_bg);
    color: #dce1ec;
}
*/
QLineEdit {
	background-color: var(lineedit_bg_color);
	border-radius: var(lineedit_radius)px;
	border: var(lineedit_border_size)px solid transparent;
	padding-left: 10px;
    padding-right: 10px;
	selection-color: var(lineedit_selection_color);
	selection-background-color: var(lineedit_context_color);
    color: var(lineedit_color);
}
QLineEdit:focus {
	border: var(lineedit_border_size)px solid var(lineedit_context_color);
    background-color: var(lineedit_bg_color_active);
	color: var(lineedit_bg_color);
}

