QTabBar::tab {
	background: var(panel_lighter_bg);
	border: 1px solid var(panel_lighter_bg);
	padding: 8px;
	font-weight: bold;
	min-width: 100px;
  }

#panel3 QTabBar::scroller{
	width: 0px;
}

QTabBar::tab:selected {
	background: var(panel_darker_bg);
	margin-bottom: -1px;
	color: #dce1ec;
	border-bottom: 2px solid var(panel_lighter_bg);;
}

QTabWidget::pane {
	/*border: 1px solid var(panel_bg);*/
	top:0px;
	background: transparent;
	border-top: 1px solid var(panel_darker_bg);
}

