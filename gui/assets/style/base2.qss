QProgressBar{
	  background: #272c36;
	  color: #fff;
}


QScrollArea, #scrollAreaWidgetContents{
    background: #191e29;
    border: 1px solid #1b202c;
}

/* Table */
QTableView{
	alternate-background-color: #191e29;
    border: 1px solid #222;
    color: #dce1ec;
	background:  #1b202c;
}

QTableCornerButton::section{
    background-color: rgb(19, 22, 31);
    border-bottom: 1px solid #222;
    border-right: 1px solid #222;
    color: #8a95aa;
}

